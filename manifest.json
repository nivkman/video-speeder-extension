{
  "name": "Video Speeder",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Video Speeder"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": { 
    "16": "logo.png",
    "48": "logo.png",
   "128": "logo.png" 
  }
}

{
  "name": "Zoom Speeder",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Zoom Speeder"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [ "<all_urls>","cookies", "tabs", "http://*/*", "https://*/*"]
}
